Algoritmo "Algoritmovelocidade"

Var
    velocidadekm1,velocidadems2: real
    


Inicio
      escreva("Insira a velocidade do veículo em km/h:")
      leia(velocidadekm1)
      
      velocidadems2 <- velocidadekm1 /3,6
      
      escreva("Esta é a velocidade do veículo em m/s:", velocidadems2)
      leia(velocidadems2)
      

      
      
      
      
      
      

Fimalgoritmo